{
  "name": "iconagency/wordpress_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "wordpress": {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  },
  "require": {
    "drupal/core-composer-scaffold": "^9.2",
    "wpackagist-plugin/fastly": "^1.2.18"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/config/application.php": "assets/config/application.php",
        "[project-root]/config/environments/all.php": {
          "mode": "replace",
          "path": "assets/config/environments/all.php",
          "overwrite": false
        },
        "[project-root]/config/environments/development.php": {
          "mode": "replace",
          "path": "assets/config/environments/development.php",
          "overwrite": false
        },
        "[project-root]/config/environments/production.php": {
          "mode": "replace",
          "path": "assets/config/environments/production.php",
          "overwrite": false
        },
        "[project-root]/.editorconfig": "assets/.editorconfig",
        "[project-root]/.vscode/extensions.json": "assets/.vscode/extensions.json",
        "[project-root]/.vscode/settings.json": {
          "mode": "replace",
          "path": "assets/.vscode/settings.json",
          "overwrite": false
        },
        "[project-root]/.phpcs.xml.dist": "assets/.phpcs.xml.dist",
        "[project-root]/README.md": {
          "mode": "replace",
          "path": "assets/README.md",
          "overwrite": false
        },
        "[project-root]/.nvmrc": {
          "mode": "replace",
          "path": "assets/.nvmrc",
          "overwrite": false
        },
        "[web-root]/index.php": "assets/web/index.php",
        "[web-root]/wp-config.php": "assets/web/wp-config.php",
        "[web-root]/app/mu-plugins/bedrock-autoloader.php": "assets/web/app/mu-plugins/bedrock-autoloader.php",
        "[web-root]/app/mu-plugins/register-theme-directory.php": "assets/web/app/mu-plugins/register-theme-directory.php"
      }
    }
  }
}
